                    <a href='@Url.Action("PrintBL", "ShipmentDetails", new { PrintName = "ORIGINAL" })' ; target="_blank">Original BL</a>
                    <a href='@Url.Action("PrintBL", "ShipmentDetails", new { PrintName = "NON-NEGOTIABLE" })' ; target="_blank">Non-Negotiable</a>
                    <a href='@Url.Action("PrintBL", "ShipmentDetails", new { PrintName = "SEAWAY" })' ; target="_blank">Seaway</a>
                    <a href='@Url.Action("PrintBL", "ShipmentDetails", new { PrintName = "SURRENDER" })' ; target="_blank">Surrender</a>
                    <a href='@Url.Action("PrintBL", "ShipmentDetails", new { PrintName = "DRAFT" })' ; target="_blank">Draft BL</a>
                </div>
            </div>
            <div class="col-md-1"></div>
            <div class="col-md-2" style="margin-top: 14px;">
                @Html.LabelFor(model => model.BLDetailsModel.NoofOriginalBLissued, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-3" style="margin-top: 14px;">
                @Html.RadioButtonFor(model => model.BLDetailsModel.NoofOriginalBLissued, "ZERO", htmlAttributes: new { @id = "NoofOriginalBLissuedZero" })
                ZERO  &nbsp;&nbsp;&nbsp;
                @Html.RadioButtonFor(model => model.BLDetailsModel.NoofOriginalBLissued, "ONE", htmlAttributes: new { @id = "NoofOriginalBLissuedOne" })
                ONE &nbsp;
                @Html.RadioButtonFor(model => model.BLDetailsModel.NoofOriginalBLissued, "ONE", htmlAttributes: new { @id = "NoofOriginalBLissuedOne" })
